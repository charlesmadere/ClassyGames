<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="BoardView">
		<attr
			format="integer"
			name="columns" />
		<attr
			format="integer"
			name="rows" />
		<attr
			format="reference"
			name="dark_background" />
		<attr
			format="reference"
			name="bright_background" />
	</declare-styleable>
	<declare-styleable name="LayoutFittingImageView">
		<attr
			format="enum"
			name="fit_dimension">
			<enum
				name="auto"
				value="0" />
			<enum
				name="landscape"
				value="1" />
			<enum
				name="portrait"
				value="2" />
		</attr>
	</declare-styleable>
</resources>