<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="Board">
		<attr
			format="integer"
			name="columns" />
		<attr
			format="integer"
			name="rows" />
	</declare-styleable>
	<declare-styleable name="BoardPosition">
		<attr
			format="integer"
			name="coordinate_x" />
		<attr
			format="integer"
			name="coordinate_y" />
	</declare-styleable>
	<declare-styleable name="LayoutFittingImageView">
		<attr
			format="enum"
			name="fitDimension">
			<enum
				name="auto"
				value="0" />
			<enum
				name="landscape"
				value="1" />
			<enum
				name="portrait"
				value="2" />
		</attr>
	</declare-styleable>
</resources>